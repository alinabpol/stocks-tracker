<table>
        <thead>
          <tr>
              <th>Ticker</th>
              <th>Name</th>
              <th>Price</th>
              <th>Actions</th>
          </tr>
        </thead>

        <tbody>
        <% @tracked_stocks.each do |stock| %>
          <tr>
            <th scope="row"><%= stock.ticker %></th>
            <td><%= stock.name %></td>
            <td><%= stock.last_price %></td>
            <td><%= link_to 'Remove', '#', user_stock_path(stock), method: :delete, data: {confirm: 'Are you sure you want to delete?'}
                                            class: 'btn btn-xs btn-warning'%></td>
          </tr>
          <% end%>
        </tbody>
</table>