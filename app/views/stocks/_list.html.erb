<table>
        <thead>
          <tr>
              <th>Ticker</th>
              <th>Name</th>
              <th>Price</th>
              <th>Actions</th>
          </tr>
        </thead>

        <tbody>
        <% @tracked_stocks.each do |stock| %>
          <tr>
            <th scope="row"><%= stock.ticker %></th>
            <td><%= stock.name %></td>
            <td><%= stock.last_price %></td>
            <td><%= 'placeholder' %></td>
          </tr>
          <% end%>
        </tbody>
</table>